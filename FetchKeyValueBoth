package p1;

import java.util.HashMap;
import java.util.Map;
import java.util.Map.Entry;
import java.util.Set;

public class FetchKeyValueBoth {
public static void main(String[] args) {
	Map map=new HashMap();
	map.put(1,"raju");
	map.put(2, "shyam");
	map.put(3, "babu");
	
	Set<Entry> set=map.entrySet();
	for(Entry reset:set)
	{
		System.out.println("key is : "+reset.getKey() +"  value is :"+reset.getValue());
	}
}
}
